        "build": "node index.js --compile-only",
        "start": "node index.js -ne -nm",
        "startnc": "node index.js --no-compile -ne -nm",
        "test": "node index.js --dev-config --dev-database",
        "testnc": "node index.js --no-compile --dev-config --dev-database"
    },
